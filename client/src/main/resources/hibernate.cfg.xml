<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
    "-//Hibernate/Hibernate Configuration DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">

<hibernate-configuration>
    <session-factory>
        <property name="hibernate.hbm2ddl.auto">create</property>
        <mapping class="com.jcalvopinam.client.model.Person" />
        <mapping class="com.jcalvopinam.client.model.ResultadoPrueba" />
        <mapping class="com.jcalvopinam.client.model.TipoParametro" />
        <mapping class="com.jcalvopinam.client.model.ResultadoEjecucion" />
    </session-factory>
</hibernate-configuration>